      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{552367A9-75FC-44DE-BE85-A325820865BC}"
          SQLTask:SqlStatementSource="CREATE TABLE [dbo].[ItemBookings]&#xA;(&#xA;[ItemBookingId] [int] NOT NULL IDENTITY(1, 1),&#xA;[ItemId] [int] NOT NULL,&#xA;[RelatedTable] [varchar] (50) COLLATE SQL_Latin1_General_CP1_CI_AS NOT NULL,&#xA;[RelatedId] [int] NOT NULL,&#xA;[BookingId] [int] NULL,&#xA;[FromDate] [datetime] NOT NULL,&#xA;[ToDate] [datetime] NULL,&#xA;[ItemBookingStatusCodeId] [int] NOT NULL,&#xA;[Quantity] [int] NOT NULL,&#xA;[InventoryStatusCodeId] [int] NOT NULL,&#xA;[IsActive] [bit] NOT NULL,&#xA;[CreatedBy] [int] NOT NULL,&#xA;[CreatedDate] [datetime] NOT NULL,&#xA;[LastUpdatedBy] [int] NOT NULL,&#xA;[LastUpdateDate] [datetime] NOT NULL&#xA;)&#xA;GO&#xA;&#xA;ALTER TABLE [dbo].[ItemBookings] ADD CONSTRAINT [PK__ItemBook__29B54B57FE2A3AB0] PRIMARY KEY CLUSTERED  ([ItemBookingId])&#xA;GO&#xA;&#xA;-- Add  ItemBriefItemId temporarily&#xA;&#xA;ALTER TABLE [dbo].[ItemBookings]&#xA;ADD ItemBriefItemId INT NOT NULL&#xA;&#xA;&#xA;--  Add itembookingid to ItemVersionHistory&#xA;ALTER TABLE [dbo].[ItemVersionHistory]&#xA;ADD ItemBookingId INT NULL&#xA;&#xA;GO&#xA;&#xA;INSERT INTO [dbo].[ItemBookings] &#xA;&#x9;(&#xA;&#x9;&#x9;[ItemId],&#xA;&#x9;&#x9;[RelatedTable], &#xA;&#x9;&#x9;[RelatedId], &#xA;&#x9;&#x9;[FromDate], &#xA;&#x9;&#x9;[ToDate], &#xA;&#x9;&#x9;[ItemBookingStatusCodeId],&#xA;&#x9;&#x9;[Quantity], &#xA;&#x9;&#x9;[InventoryStatusCodeId], &#xA;&#x9;&#x9;[IsActive], &#xA;&#x9;&#x9;[CreatedBy], &#xA;&#x9;&#x9;[CreatedDate], &#xA;&#x9;&#x9;[LastUpdatedBy],&#xA;&#x9;&#x9;[LastUpdateDate],&#xA;&#x9;&#x9;ItemBriefItemId ) &#xA;SELECT&#x9;[ItemId], &#xA;&#x9;&#x9;'ItemBrief', &#xA;&#x9;&#x9;ISNULL([ItemBriefId], 0),&#xA;&#x9;&#x9;ISNULL([FromDate],[CreatedDate]) ,&#xA;&#x9;&#x9;[ToDate],&#xA;&#x9;&#x9;[ItemBriefItemStatusCodeId],&#xA;&#x9;&#x9;[Quantity],&#xA;&#x9;&#x9;[BookingStatusCodeId],&#xA;&#x9;&#x9;[IsActive], &#xA;&#x9;&#x9;ISNULL([CreatedBy], 0), &#xA;&#x9;&#x9;[CreatedDate], &#xA;&#x9;&#x9;ISNULL([LastUpdatedBy], 0),&#xA;&#x9;&#x9;ISNULL([LastUpdateDate], [CreatedDate]),&#xA;&#x9;&#x9;[ItemBriefItemId]&#xA;FROM&#x9;[dbo].[ItemBriefItem]&#xA;&#xA;-- Insert booking id from bookings table to ItemBookings table&#xA;&#xA;declare @BookingId as int&#xA;declare @RelatedId as int&#xA;declare @RelatedTable as varchar(100)&#xA;&#xA;DECLARE booking_cursor CURSOR FAST_FORWARD FOR&#xA;&#xA;select Bookingid,RelatedId,RelatedTable &#xA;from &#xA;Bookings b&#xA;&#xA;OPEN booking_cursor&#xA;FETCH NEXT FROM booking_cursor &#xA;INTO @BookingId,@RelatedId,@RelatedTable&#xA;&#x9;&#x9;&#xA;WHILE @@FETCH_STATUS = 0&#xA;BEGIN&#xA;&#x9;if(@RelatedTable = 'Project')&#xA;&#x9;begin&#xA;&#x9;&#x9;update ItemBookings&#xA;&#x9;&#x9;set BookingId = @BookingId&#xA;&#x9;&#x9;from &#xA;&#x9;&#x9;ItemBookings ibs&#xA;&#x9;&#x9;inner join ItemBriefs ib on ib.ItemBriefId = ibs.RelatedId&#xA;&#x9;&#x9;where ib.ProjectId = @RelatedId&#xA;&#x9;end&#xA;&#xA;&#xA;FETCH NEXT FROM booking_cursor INTO @BookingId,@RelatedId,@RelatedTable&#xA;END &#xA;CLOSE booking_cursor;&#xA;DEALLOCATE booking_cursor;&#xA;&#xA;-- relpace itembriefitemid&#xA;&#xA;UPDATE [dbo].[ItemVersionHistory]&#xA;SET [ItemBookingId] = ib.ItemBookingId&#xA;FROM [dbo].[ItemVersionHistory] ivh&#xA;&#x9;INNER JOIN [dbo].[ItemBookings] ib ON ivh.ItembriefItemId = ib.ItemBriefItemId&#xA;&#xA;ALTER TABLE [dbo].[InventoryActivity]&#xA;ADD ItemBookingId INT NOT NULL default(0)&#xA;GO&#xA;&#xA;UPDATE [dbo].[InventoryActivity]&#xA;SET [ItemBookingId] = ib.ItemBookingId&#xA;FROM [dbo].[InventoryActivity] ia&#xA;&#x9;INNER JOIN [dbo].[ItemBookings] ib ON ia.[ItemBriefItemId] = ib.ItemBriefItemId&#xA;&#xA;&#xA;-- move companyid and bookingnumber from booking table to new companybooking number table&#xA;&#xA;Insert into CompanyBookingNumbers(BookingNumber,CompanyId,BookingId,CreatedDate)&#xA;select BookingNumber, CompanyId,Bookingid, CreatedDate&#xA;from Bookings&#xA;&#xA;-- Add companybookingnumber to DailyBookingNotifications&#xA;&#xA;ALTER TABLE [dbo].[DailyBookingNotifications] &#xA;ADD [CompanyBookingNumberId] INT NOT NULL&#xA;&#xA;GO&#xA;&#xA;UPDATE [dbo].[DailyBookingNotifications]&#xA;SET [CompanyBookingNumberId] = cbn.CompanyBookingNumberId&#xA;FROM [dbo].[DailyBookingNotifications] dbn&#xA; INNER JOIN [dbo].[CompanyBookingNumbers] cbn ON dbn.BookingId = cbn.BookingId&#xA;&#xA;----------------------------&#x9;&#xA;--ALTER TABLE [dbo].[DailyBookingNotifications] &#xA;--DROP COLUMN [BookingId]&#xA;&#xA;--ALTER TABLE [dbo].[ItemVersionHistory] &#xA;--DROP COLUMN ItemBriefItemId&#xA;&#xA;--ALTER TABLE [dbo].[ItemBookings] &#xA;--DROP COLUMN ItemBriefItemId&#xA;&#xA;GO" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
