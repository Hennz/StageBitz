      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{B9655067-3205-4AA1-8BC5-4F4518727F50}"
          SQLTask:SqlStatementSource="DECLARE @inventoryObserverCodeId INT= [dbo].[GetCodeIDByCodeValue]('CompanyUserTypeCode', 'INVOBSERVER')&#xA;DECLARE @inventoryStaffCodeId INT = [dbo].[GetCodeIDByCodeValue]('CompanyUserTypeCode', 'INVSTAFF')&#xA;&#xA;INSERT INTO [dbo].[CompanyUserRole] &#xA;SELECT&#x9;cur.[CompanyUserId]&#xA;      ,cur.[CompanyUserTypeCodeId]&#xA;      ,cur.[CreatedByUserId]&#xA;      ,cur.[CreatedDate]&#xA;      ,cur.[LastUpdatedByUserId]&#xA;      ,cur.[LastUpdatedDate]&#xA;      ,cur.[IsActive]&#xA;&#x9;  ,l.LocationId&#xA;  FROM [dbo].[CompanyUserRole] cur&#xA;&#x9;INNER JOIN [dbo].[CompanyUsers] cu ON cur.[CompanyUserId] = cu.[CompanyUserId]&#xA;&#x9;INNER JOIN [dbo].[Locations] l ON cu.CompanyId = l.CompanyId AND l.IsActive = 1 AND l.ParentLocationId IS NULL&#xA;  where cur.IsActive = 1 &#xA;&#x9;AND ([CompanyUserTypeCodeId] = @inventoryObserverCodeId OR [CompanyUserTypeCodeId] = @inventoryStaffCodeId) &#xA;&#x9;AND cur.LocationId IS NULL&#xA;&#xA;DELETE [dbo].[CompanyUserRole]&#xA;WHERE IsActive = 1 &#xA;&#x9;AND ([CompanyUserTypeCodeId] = @inventoryObserverCodeId OR [CompanyUserTypeCodeId] = @inventoryStaffCodeId) &#xA;&#x9;AND LocationId IS NULL &#xA;&#xA;INSERT INTO [dbo].[InvitationUserRole]&#xA;SELECT&#x9;iur.[InvitationId]&#xA;&#x9;,iur.[UserTypeCodeId]&#xA;&#x9;,iur.[CreatedByUserId]&#xA;    ,iur.[CreatedDate]&#xA;    ,iur.[LastUpdatedByUserId]&#xA;    ,iur.[LastUpdatedDate]&#xA;    ,iur.[IsActive]&#xA;&#x9;,l.LocationId&#xA;FROM [dbo].[InvitationUserRole] iur &#xA;&#x9;INNER JOIN [dbo].[Invitations] i ON i.[InvitationId] = iur.[InvitationId]&#xA;&#x9;&#x9;INNER JOIN [dbo].[Locations] l ON i.[RelatedId] = l.CompanyId AND l.IsActive = 1 AND l.ParentLocationId IS NULL&#xA;WHERE [RelatedTable] = 'Companies' AND (iur.[UserTypeCodeId] = @inventoryObserverCodeId OR iur.[UserTypeCodeId] = @inventoryStaffCodeId) &#xA;&#x9;AND iur.LocationId IS NULL &#xA;&#xA;DELETE [dbo].[InvitationUserRole]&#xA;WHERE IsActive = 1 &#xA;&#x9;AND ([UserTypeCodeId] = @inventoryObserverCodeId OR [UserTypeCodeId] = @inventoryStaffCodeId) &#xA;&#x9;AND LocationId IS NULL &#xA;&#xA;&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
