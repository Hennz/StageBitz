      DTS:refId="Package\Update Inventory Roles"
      DTS:CreationName="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{D49E0AAF-5DA0-44F5-9600-A6B9E7D51CCB}"
      DTS:ExecutableType="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:LocaleID="-1"
      DTS:ObjectName="Update Inventory Roles"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{6F877296-3AD5-41C2-A43E-3413F6875203}"
          SQLTask:SqlStatementSource="Declare @companyId INT&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;DECLARE @PCAdminCode INT = [dbo].[GetCodeIDByCodeValue]('CompanyUserTypeCode', 'ADMIN')&#xA;&#x9;DECLARE @InventoryManagerCode INT = [dbo].[GetCodeIDByCodeValue]('CompanyUserTypeCode', 'INVMGR')&#xA;&#x9;DECLARE @InventoryStaffCode INT = [dbo].[GetCodeIDByCodeValue]('CompanyUserTypeCode', 'INVSTAFF')&#xA;&#x9;DECLARE @InventoryAdminCode INT = [dbo].[GetCodeIDByCodeValue]('CompanyUserTypeCode', 'INVADMIN')&#xA;&#xA;&#x9;DECLARE c_cursor CURSOR FOR&#x9;&#xA;&#x9;select companyId from [dbo].[Companies]&#xA;&#x9;OPEN c_cursor&#xA;&#x9;FETCH NEXT FROM c_cursor&#xA;&#x9;INTO @companyId&#xA;&#xA;&#x9;WHILE @@FETCH_STATUS = 0 &#xA;&#x9;&#x9;BEGIN&#xA;&#xA;&#x9;&#x9;&#x9;DECLARE @TmpUserInfo TABLE (&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;CompanyUserId INT,&#xA;&#x9;&#x9;&#x9;&#x9;CompanyUserRoleId INT,&#xA;&#x9;&#x9;&#x9;&#x9;CompanyUserTypeCodeId INT,&#xA;&#x9;&#x9;&#x9;&#x9;UserId INT&#xA;&#x9;&#x9;&#x9;)&#xA;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;INSERT INTO @TmpUserInfo&#xA;&#x9;&#x9;&#x9;select cu.CompanyUserId, cur.CompanyUserRoleId, cur.CompanyUserTypeCodeId, cu.UserId&#xA;&#x9;&#x9;&#x9;from [dbo].[CompanyUserRole] cur&#xA;&#x9;&#x9;&#x9;&#x9;INNER JOIN [dbo].[CompanyUsers] cu on cur.CompanyUserId = cu.CompanyUserId&#xA;&#x9;&#x9;&#x9;Where cu.CompanyId = @companyId AND cur.IsActive = 1 AND cu.IsActive = 1&#xA;&#xA;&#x9;&#x9;&#x9;DECLARE @PACompanyUser INT = 0&#xA;&#x9;&#x9;&#x9;SELECT @PACompanyUser = CompanyUserId&#xA;&#x9;&#x9;&#x9;FROM @TmpUserInfo&#xA;&#x9;&#x9;&#x9;WHERE CompanyUserTypeCodeId = @PCAdminCode&#xA;&#xA;&#x9;&#x9;&#x9;DECLARE @IMCount INT = 0&#xA;&#x9;&#x9;&#x9;SELECT @IMCount = COUNT(*) FROM @TmpUserInfo WHERE CompanyUserTypeCodeId = @InventoryManagerCode&#xA;&#x9;&#x9;&#x9;IF (@IMCount = 1)&#xA;&#x9;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;UPDATE [dbo].[CompanyUserRole]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;SET CompanyUserTypeCodeId = @InventoryAdminCode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE CompanyUserRoleId = (&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT CompanyUserRoleId FROM @TmpUserInfo WHERE CompanyUserTypeCodeId = @InventoryManagerCode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xA;&#x9;&#x9;&#x9;END&#xA;&#x9;&#x9;&#x9;ELSE IF (@IMCount &gt; 1)&#xA;&#x9;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;&#x9;DECLARE @AdminUserID INT = 0&#xA;&#x9;&#x9;&#x9;    DECLARE @AdminCount INT = 0&#xA;&#x9;&#x9;&#x9;&#x9;SELECT @AdminCount = COUNT(*), @AdminUserID = MAX(UserId) FROM @TmpUserInfo &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE CompanyUserTypeCodeId &lt;&gt; @InventoryManagerCode &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND UserId IN (SELECT UserId FROM @TmpUserInfo WHERE CompanyUserTypeCodeId = @InventoryManagerCode)&#xA;&#xA;&#x9;&#x9;&#x9;&#x9;IF(@AdminCount = 1)&#xA;&#x9;&#x9;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;-- Update non company admins as inventory staff&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;UPDATE [dbo].[CompanyUserRole]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;SET CompanyUserTypeCodeId = @InventoryStaffCode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE CompanyUserRoleId IN (&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT CompanyUserRoleId FROM @TmpUserInfo WHERE CompanyUserTypeCodeId = @InventoryManagerCode AND UserId &lt;&gt; @AdminUserID&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xA;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;-- Update CA as IA&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;UPDATE [dbo].[CompanyUserRole]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;SET CompanyUserTypeCodeId = @InventoryAdminCode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE CompanyUserRoleId IN (&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT CompanyUserRoleId FROM @TmpUserInfo WHERE CompanyUserTypeCodeId = @InventoryManagerCode AND UserId = @AdminUserID&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xA;&#xA;&#x9;&#x9;&#x9;&#x9;END&#xA;&#x9;&#x9;&#x9;&#x9;ELSE&#xA;&#x9;&#x9;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;IF(@AdminCount = 0)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;UPDATE [dbo].[CompanyUserRole]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SET CompanyUserTypeCodeId = @InventoryStaffCode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE CompanyUserRoleId IN (&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT CompanyUserRoleId FROM @TmpUserInfo WHERE CompanyUserTypeCodeId = @InventoryManagerCode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#xA;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;END&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;UPDATE [dbo].[CompanyUserRole]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SET IsActive = 0&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE CompanyUserRoleId IN (&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT CompanyUserRoleId FROM @TmpUserInfo WHERE CompanyUserTypeCodeId = @InventoryManagerCode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;)&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;END&#xA;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;IF EXISTS (SELECT * FROM @TmpUserInfo WHERE CompanyUserId = @PACompanyUser AND CompanyUserTypeCodeId = @InventoryManagerCode)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; UPDATE [dbo].[CompanyUserRole]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; SET CompanyUserTypeCodeId = @InventoryAdminCode, IsActive = 1&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; WHERE CompanyUserId = @PACompanyUser AND CompanyUserTypeCodeId = @InventoryStaffCode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;END&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;BEGIN&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;INSERT INTO [dbo].[CompanyUserRole]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ([CompanyUserId]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,[CompanyUserTypeCodeId]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,[CreatedByUserId]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,[CreatedDate]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,[LastUpdatedByUserId]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,[LastUpdatedDate]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,[IsActive])&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; VALUES&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   (@PACompanyUser&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,@InventoryAdminCode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,0&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,[dbo].[GetSystemDate]()&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,0&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,[dbo].[GetSystemDate]()&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,1)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;END&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;END &#xA;&#x9;&#x9;&#x9;END&#xA;&#x9;&#x9;&#x9;ELSE&#xA;&#x9;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;    INSERT INTO [dbo].[CompanyUserRole]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ([CompanyUserId]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,[CompanyUserTypeCodeId]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,[CreatedByUserId]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,[CreatedDate]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,[LastUpdatedByUserId]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,[LastUpdatedDate]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,[IsActive])&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; VALUES&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   (@PACompanyUser&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,@InventoryAdminCode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,0&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,[dbo].[GetSystemDate]()&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,0&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,[dbo].[GetSystemDate]()&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   ,1)&#xA;&#x9;&#x9;&#x9;END&#xA;&#xA;&#x9;&#x9;&#x9;DELETE FROM @TmpUserInfo&#xA;&#xA;&#x9;&#x9;FETCH NEXT FROM c_cursor INTO @companyId&#xA;&#x9;&#x9;END&#xA;&#x9;CLOSE c_cursor&#xA;&#x9;DEALLOCATE c_cursor&#xA;&#xA;&#x9;UPDATE [dbo].[CompanyUserRole]&#xA;&#x9;SET CompanyUserTypeCodeId = @InventoryStaffCode&#xA;&#x9;WHERE CompanyUserTypeCodeId = @InventoryManagerCode AND IsActive = 0&#xA;&#xA;&#x9;UPDATE [dbo].[InvitationUserRole]&#xA;&#x9;SET UserTypeCodeId = @InventoryStaffCode&#xA;&#x9;WHERE UserTypeCodeId = @InventoryManagerCode" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
