    <DTS:Executable
      DTS:refId="Package\Migrate SPP companies to Shared Companies"
      DTS:CreationName="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{36BDFCA7-7202-46A6-B319-BD1CAEF9A16E}"
      DTS:ExecutableType="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:LocaleID="-1"
      DTS:ObjectName="Migrate SPP companies to Shared Companies"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{6FDC4195-5C2E-4BC9-BD72-89035C09D40D}"
          SQLTask:SqlStatementSource="declare @companyId as int&#xA;declare @ActivecompanyInventorySharingStatusCodeId as int &#xA;set @ActivecompanyInventorySharingStatusCodeId = [dbo].[GetCodeIDByCodeValue]('CompanyInventorySharingStatus','ACTIVE')&#xA;&#xA;begin tran&#xA;&#xA;&#x9;DECLARE sppCompany_cursor CURSOR FOR &#xA;&#x9;SELECT CompanyId&#xA;&#x9;FROM &#xA;&#x9;SPPComapnies&#xA;&#x9;WHERE IsActive = 1&#xA;&#xA;&#xA;&#x9;OPEN sppCompany_cursor&#xA;&#xA;&#x9;FETCH NEXT FROM sppCompany_cursor &#xA;&#x9;INTO @companyId&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;WHILE @@FETCH_STATUS = 0&#xA;&#x9;BEGIN&#xA;&#xA;&#x9;Insert Into CompanyInventorySharingDetails(CompanyId,[ViewingCompanyId],[CompanySharingStatusCodeId],[CreatedByUserId],[CreatedDate],[LastUpdatedByUserId],[LastUpdatedDate])&#xA;&#x9;Select @companyId, CompanyId ,@ActivecompanyInventorySharingStatusCodeId,0,GETDATE(),0,GETDATE() &#x9;&#xA;&#x9;From SPPComapnies&#xA;&#x9;where CompanyId !=  @companyId&#xA;&#xA;&#x9;------------------------------ Remove Watch List Items from SPP Company --------------------------------------------------&#xA;&#x9;&#x9;&#xA;&#x9;IF EXISTS (select count(*) from dbo.SPPComapnies where CompanyId = @CompanyId)&#xA;&#x9;begin&#x9;&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;declare @WatchListHeaderId as int&#xA;&#x9;&#x9;declare @WatchListItemId as int&#x9;&#x9;&#xA;&#x9;&#x9;DECLARE @WatchListHeaderCompanyId INT&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;--The Watch List of users who does not belongs to the company which is going to be deleted.&#xA;&#x9;&#x9;-- Delete WatchList items that came from other companies.&#xA;&#x9;&#x9;DECLARE watchHeader_cursorByOther CURSOR FOR&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;SELECT&#x9; WatchListHeaderId&#xA;&#x9;&#x9;FROM dbo.WatchListHeader&#xA;&#x9;&#x9;WHERE CompanyId &lt;&gt; @CompanyId&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;OPEN watchHeader_cursorByOther&#xA;&#x9;&#x9;FETCH NEXT FROM watchHeader_cursorByOther &#xA;&#x9;&#x9;INTO @WatchListHeaderId&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;WHILE @@FETCH_STATUS = 0&#xA;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;-----------&#xA;&#x9;&#x9;&#x9;DECLARE watchItem_cursor CURSOR FOR&#xA;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;SELECT&#x9; WatchListItemId&#xA;&#x9;&#x9;&#x9;FROM dbo.WatchListItems&#xA;&#x9;&#x9;&#x9;left outer join dbo.Items on Items.ItemId = WatchListItems.ItemId&#xA;&#x9;&#x9;&#x9;WHERE Items.CompanyId = @CompanyId and WatchListHeaderId = @WatchListHeaderId&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;OPEN watchItem_cursor&#xA;&#x9;&#x9;&#x9;FETCH NEXT FROM watchItem_cursor &#xA;&#x9;&#x9;&#x9;INTO @WatchListItemId&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;WHILE @@FETCH_STATUS = 0&#xA;&#x9;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;--Delete Watch List Items&#xA;&#x9;&#x9;&#x9;&#x9;delete from WatchListItems where WatchListItemId = @WatchListItemId&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;FETCH NEXT FROM watchItem_cursor INTO @WatchListItemId&#xA;&#x9;&#x9;&#x9;END &#xA;&#x9;&#x9;&#x9;CLOSE watchItem_cursor;&#xA;&#x9;&#x9;&#x9;DEALLOCATE watchItem_cursor;&#xA;&#x9;&#x9;&#x9;-----------&#x9;&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;-- If Watch List Header contains no WatchList items, Delete that as well.&#xA;&#x9;&#x9;&#x9;IF NOT EXISTS (select count(*) from dbo.WatchListItems where WatchListHeaderId = @WatchListHeaderId)&#xA;&#x9;&#x9;&#x9;begin&#xA;&#x9;&#x9;&#x9;&#x9;-- delete command&#xA;&#x9;&#x9;&#x9;&#x9;delete from dbo.WatchListHeader where WatchListHeaderId = @WatchListHeaderId&#xA;&#x9;&#x9;&#x9;end&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;FETCH NEXT FROM watchHeader_cursorByOther INTO @WatchListHeaderId&#xA;&#x9;&#x9;END &#xA;&#x9;&#x9;CLOSE watchHeader_cursorByOther;&#xA;&#x9;&#x9;DEALLOCATE watchHeader_cursorByOther;&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;--The Watch List of users who belongs to the Company which is going to be deleted&#xA;&#x9;&#x9;-- Delete WatchList items that they have added from other companies.&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;SET @WatchListHeaderId = 0&#xA;&#x9;&#x9;SET @WatchListItemId = 0&#xA;&#x9;&#x9;SET @WatchListHeaderCompanyId = 0&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;DECLARE watchHeader_cursorFromOther CURSOR FOR&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;SELECT&#x9; WatchListHeaderId,CompanyId&#xA;&#x9;&#x9;FROM dbo.WatchListHeader&#xA;&#x9;&#x9;WHERE CompanyId = @CompanyId&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;OPEN watchHeader_cursorFromOther&#xA;&#x9;&#x9;FETCH NEXT FROM watchHeader_cursorFromOther &#xA;&#x9;&#x9;INTO @WatchListHeaderId, @WatchListHeaderCompanyId&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;WHILE @@FETCH_STATUS = 0&#xA;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;-----------&#xA;&#x9;&#x9;&#x9;DECLARE watchItem_cursor CURSOR FOR&#xA;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;SELECT&#x9; WatchListItemId&#xA;&#x9;&#x9;&#x9;FROM dbo.WatchListItems&#xA;&#x9;&#x9;&#x9;left outer join dbo.Items on Items.ItemId = WatchListItems.ItemId&#xA;&#x9;&#x9;&#x9;WHERE Items.CompanyId &lt;&gt; @WatchListHeaderCompanyId and WatchListHeaderId = @WatchListHeaderId&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;OPEN watchItem_cursor&#xA;&#x9;&#x9;&#x9;FETCH NEXT FROM watchItem_cursor &#xA;&#x9;&#x9;&#x9;INTO @WatchListItemId&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;WHILE @@FETCH_STATUS = 0&#xA;&#x9;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;--Delete Watch List Items&#xA;&#x9;&#x9;&#x9;&#x9;delete from WatchListItems where WatchListItemId = @WatchListItemId&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;FETCH NEXT FROM watchItem_cursor INTO @WatchListItemId&#xA;&#x9;&#x9;&#x9;END &#xA;&#x9;&#x9;&#x9;CLOSE watchItem_cursor;&#xA;&#x9;&#x9;&#x9;DEALLOCATE watchItem_cursor;&#xA;&#x9;&#x9;&#x9;-----------&#x9;&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;-- If Watch List Header contains no WatchList items, Delete that as well.&#xA;&#x9;&#x9;&#x9;IF NOT EXISTS (select count(*) from dbo.WatchListItems where WatchListHeaderId = @WatchListHeaderId)&#xA;&#x9;&#x9;&#x9;begin&#xA;&#x9;&#x9;&#x9;&#x9;-- delete command&#xA;&#x9;&#x9;&#x9;&#x9;delete from dbo.WatchListHeader where WatchListHeaderId = @WatchListHeaderId&#xA;&#x9;&#x9;&#x9;end&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;FETCH NEXT FROM watchHeader_cursorFromOther INTO @WatchListHeaderId, @WatchListHeaderCompanyId&#xA;&#x9;&#x9;END &#xA;&#x9;&#x9;CLOSE watchHeader_cursorFromOther;&#xA;&#x9;&#x9;DEALLOCATE watchHeader_cursorFromOther;&#x9;&#xA;&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;end&#xA;&#xA;&#x9;---------------------------------------------------------------------------------------------------------------------------&#xA;&#xA;&#xA;&#x9;FETCH NEXT FROM sppCompany_cursor INTO @companyId   &#xA;&#x9;END &#xA;&#xA;&#x9;CLOSE sppCompany_cursor;&#xA;&#x9;DEALLOCATE sppCompany_cursor;&#xA;&#x9;&#x9;&#xA;&#x9;--Delete the Company record from the SPPCompanies Table&#xA;&#x9;DELETE FROM dbo.SPPComapnies&#xA;&#x9;&#xA;-- Rollback the transaction if there were any errors&#xA;IF(@@error&gt;0)&#xA;BEGIN&#xA;&#x9;ROLLBACK TRAN&#xA;END&#xA;ELSE&#xA;BEGIN&#x9;&#xA;&#x9;COMMIT TRAN&#xA;END&#xA;&#xA;&#xA;&#x9;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
