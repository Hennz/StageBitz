      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{6A3393B8-750C-4153-AE49-138C275FE778}"
          SQLTask:SqlStatementSource="BEGIN TRANSACTION&#xA;&#xA;&#x9;-- Has ot initialise to 0, else need to skip the first record&#xA;&#x9;update Bookings set BookingNumber = 0&#xA;&#xA;&#x9;declare @BookingId as int&#xA;&#x9;declare @CompanyId as int&#xA;&#x9;declare @CurrentCompanyId as int set @CurrentCompanyId = 0&#xA;&#x9;declare @MaxBookingNumber as int set @MaxBookingNumber = 0&#xA;&#xA;&#x9;DECLARE Booking_cursor CURSOR FOR&#xA;&#xA;&#x9;select BookingId,CompanyId from Bookings order by CompanyId&#xA;&#x9;&#xA;&#x9;OPEN Booking_cursor&#xA;&#x9;FETCH NEXT FROM Booking_cursor &#xA;&#x9;INTO @BookingId,@CompanyId&#xA;&#x9;&#xA;&#x9;WHILE @@FETCH_STATUS = 0&#xA;&#x9;BEGIN&#xA;&#xA;&#x9;if(@CurrentCompanyId != @CompanyId)&#xA;&#x9;begin&#x9;&#xA;&#x9;&#x9;set @CurrentCompanyId = @CompanyId&#xA;&#x9;&#x9;select  @MaxBookingNumber = max(BookingNumber) from Bookings where CompanyId = @CompanyId&#xA;&#x9;end&#xA;&#x9;&#xA;&#x9;set @MaxBookingNumber = @MaxBookingNumber+1&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;--select @MaxBookingNumber, @CompanyId&#xA;&#x9;update Bookings set BookingNumber = @MaxBookingNumber  where BookingId = @BookingId&#xA;&#x9;&#xA;&#x9;FETCH NEXT FROM Booking_cursor INTO @BookingId,@CompanyId&#xA;&#x9;END &#xA;&#x9;CLOSE Booking_cursor;&#xA;&#x9;DEALLOCATE Booking_cursor;&#xA;&#x9;&#xA;&#xA;&#x9;-- Rollback the transaction if there were any errors&#xA;&#x9;IF(@@error&gt;0)&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;ROLLBACK TRAN&#xA;&#x9;END&#xA;&#x9;ELSE&#xA;&#x9;BEGIN&#x9;&#xA;&#x9;&#x9;COMMIT TRAN&#xA;END&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
