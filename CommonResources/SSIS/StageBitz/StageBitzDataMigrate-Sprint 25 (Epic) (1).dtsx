          SQLTask:Connection="{6A3393B8-750C-4153-AE49-138C275FE778}"
          SQLTask:SqlStatementSource="DECLARE @Date DATETIME set @Date = [dbo].[GetSystemDate]();&#x9;&#xA;&#xA;-- Insert Bookings records&#xA;INSERT INTO [dbo].[Bookings]&#xA;([RelatedTable], [RelatedId],CompanyId, [CreatedByUserId], [CreatedDate], [LastUpdatedByUserId], [LastUpdatedDate])&#xA;SELECT DISTINCT 'Project', ib.ProjectId, i.CompanyId , 0, @Date, 0, @Date&#xA;FROM [dbo].[ItemBriefItem] ibi&#xA;&#x9;INNER JOIN [dbo].[ItemBriefs] ib ON ib.[ItemBriefId] = ibi.[ItemBriefId]&#xA;&#x9;INNER JOIN [dbo].[Items] i ON i.ItemId = ibi.ItemId&#xA;WHERE ib.ProjectId not in (SELECT [RelatedId] FROM [dbo].[Bookings])&#xA;&#xA;&#xA;DECLARE @NewStatusCodeId INT = [dbo].[GetCodeIDByCodeValue]('ItemBriefItemBookingStatus', 'NOTPICKEDUP')&#xA;&#xA;UPDATE [dbo].[ItemBriefItem]&#xA;SET [BookingStatusCodeId] = @NewStatusCodeId" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
